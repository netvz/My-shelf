<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="320.0" style="-fx-background-color: pink;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ProductPage">
   <children>
      <Button fx:id="sb" layoutX="70.0" layoutY="22.0" mnemonicParsing="false" onAction="#searchbutton" prefHeight="32.0" prefWidth="176.0" text="Display My products" />
      <Button fx:id="backButton" layoutX="131.0" layoutY="352.0" mnemonicParsing="false" onAction="#gotomenu" text="Back" />
      <ScrollPane layoutX="64.0" layoutY="76.0">
         <content>
            <TextArea fx:id="displayProducts" prefHeight="240.0" prefWidth="189.0" />
         </content>
      </ScrollPane>
      <Button fx:id="delbutton" layoutX="228.0" layoutY="339.0" mnemonicParsing="false" onAction="#deleteButton" text="Delete" />
   </children>
</AnchorPane>
